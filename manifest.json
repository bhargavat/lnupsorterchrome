{
  "manifest_version": 2,

  "name": "LineupSorter",
  "short_name": "lnupsorter",
  "description": "Allows you to quickly sort your Yahoo Fantasy Basketball roster for the week.",
  "version": "1.2.1",

  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
   "default_icon": "img/icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
